.wx-card-container{
  margin-top: 15px;
  height: 118px;
  width: calc(100% - 60rpx);
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-radius: 6px;
  background:linear-gradient(#45B649,#6FCC3C);
  position: absolute;
  
}
.wx-card__header{
  padding-top: 15px;
  display: flex;
  align-items: center;
}

.wx-card__header__icon{
  margin-left: 19px;
  width: 20px;
  height: 20px;
}
.wx-card__header__title{
  margin-left: 14px;
  font-size: 16px;
  color: white;
}

.wx-card__mid{
  display: flex;
  align-items: center;
  margin-top: 13px;
}
.wx-card__mid__num1{
  margin-left: 38rpx;
  font-size: 48rpx;
  color: white;
  margin-right: 70rpx;
  width: 96rpx;
}
.wx-card__mid__dot{
  margin-right: 10rpx;
  width: 16rpx;
  height: 16rpx;
  border-radius: 8rpx;
  background-color: rgba(255,255,255,0.4);
}
.wx-card__mid__dot-last{
  margin-right: 0rpx;
}

.wx-card__mid__num2{
  margin-left: 60rpx;
  font-size: 48rpx;
   color: white;
}

.wx-card__watermark{
  position: absolute;
  bottom: 0px;
  right: 0px;
  width: 147px;
  height: 85px;
}