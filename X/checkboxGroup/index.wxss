/* X/checkboxGroup/index.wxss */
.x-cell{
  width: 100%;
  height: 44px;
  display: flex;
  align-items: center;
  border-bottom: 1px solid rgb(247,247,247);
}
.x-check{
  display: none;
}
.x-icon-checkbox{
  margin-left: 13px;
}
.x-cell__icon{
  margin-left: 10px;
  width: 26px;
  height: 26px;
}
.x-cell__title{
  margin-left: 10px;
}
.x-cell__count{
  margin-right: 15px;
  font-size: 15px;
  color: rgb(100,100,100);
}