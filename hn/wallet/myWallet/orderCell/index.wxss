.order-cell{
  width: 100%;
  box-sizing: border-box;
  padding-left: 50rpx;
  padding-right: 50rpx;
  background: white;
}
.order-cell:active{
  background: rgb(250,250,250);
}
.order-num{
  padding-top: 30rpx;
  font-size: 30rpx;
  color: #333333;
}
.order-flex{
  flex: 1;
}
.order__mid{
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}
.order__mid__plate-number{
  font-size:24rpx;
  color: #999999;
}
.order__mid__arrow{
  margin-left: 20rpx;
  width:12rpx;
  height: 20rpx;
}
.order__mid__price{
  font-size: 30rpx;
  color: #333333;
}
.order-date{
  margin-top: 10rpx;
  color: #999999;
  font-size: 24rpx;
}
.order-line{
  margin-top: 30rpx;
  height: 2rpx;
  background-color: #EEEEEE;

}